{
  "compilerOptions": {
    "target": "ES2023",  
    "module": "CommonJS",
    "outDir": "./build",
    "checkJs":true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": true,
    "moduleResolution": "Node",
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "strict": true,     
    "skipLibCheck": true     
}
}